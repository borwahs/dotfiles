alias l='ls'
alias ll='ls -al'
alias lh='ls -Alh'

# display files as list on each row
alias lns='ls -L | tr -s " "'

alias owc='open *.xcworkspace'

alias path='echo $PATH | tr -s ":" "\n"'

alias od='cd ~/Development'

# Include custom aliases
if [ -f ~/.aliases.local ]; then
    source ~/.aliases.local
fi
